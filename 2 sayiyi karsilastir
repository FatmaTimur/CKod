#include <iostream>
using namespace std;

int main () {

    int sayi1;
    int sayi2;
    
    cout <<"iki sayi giriniz : ";
    cin >> sayi1 >> sayi2;

    if (sayi1 == sayi2)
        cout << "sayilar esittir" <<endl;

    if (sayi1 != sayi2)
        cout << "sayilar esit degil" << endl;

    if (sayi1 > sayi2)
        cout << "1.sayi 2. sayidan buyuktur" << endl;

    if (sayi1 < sayi2)
        cout << "1. sayi 2.sayidan kucuktur" << endl;

    if (sayi1 >= sayi2)
        cout << "1. sayi buyuk yada esittir 2.sayiya" << endl;

    if (sayi1 <= sayi2)
        cout << "1. sayi kucuk yada esittir 2.sayiya" << endl;

}
